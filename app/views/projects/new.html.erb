<h1 class="center top-offset">Create new project</h1>


<div class="row">
  <div class="span6 offset3">
    <%= form_for(@project) do |f| %>
    <%= render 'shared/project_error_messages' %>
    <%= f.label :project_name %>
    <%= f.text_field :project_name, placeholder:"Project name" %>

    <%= f.submit "Create project", class: "btn btn-large btn-custom" %>

    <% end %>
  </div>
</div>